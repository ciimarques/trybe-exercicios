let numbers = [5, 9, 3, 19, 70, 8, 100, 2, 35, 27];
let soma = 0
let maior = 0
for(let number of numbers){
    console.log(number)
    soma = soma + number
    if (number > maior){
        maior = number
    }
}
console.log(soma)

let media = soma / numbers.length

console.log(media);
if(media > 20){
   console.log('O valor da média aritmética é maior que 20')
} else {
    console.log('O valor da média aritmética é menor ou igual a 20')
}

console.log(maior)

let menor = 0
for(let number of numbers){
    console.log(number)
    subtrair = subtrair - number
    if (menor < number){
        menor = number
    }
}
console.log(subtrair)







